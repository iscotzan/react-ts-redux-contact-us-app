{
  "extends": [],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "**/*.test.tsx"
    ],
    "rules": {
      "interface-name": [true, "never-prefix"],
      "object-literal-sort-keys": false,
      "member-ordering": false,
      "jsx-no-lambda": false,
      "no-console": false,
      "ordered-imports": false
    }
  }
}
